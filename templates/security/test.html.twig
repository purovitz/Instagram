
{% for post in posts %}

    <br>

    <div class="container">
        <div class="row card-body">
            <div class="col-md-12 ">
                <div class="wrapper text-center">
                    <img src="/images/hosting/{{ post.image }}" class="card-img-top mt-4 image" alt="..." style="width:17cm; height: 5cm">
                    <div class="container">
                        <div class="card mt-5">
                            <div class="card-body ">
                                <p>{{post.content}}</p>
